.content-title
    .content-title__header.h5 {{ 'TIMELINE.EVENTS.' + selectedTab.eventType | translate }}
        ng-container([ngSwitch]="selectedTab.eventType")
            ng-container(*ngSwitchCase="EventType.SEND_DOCUMENT") : {{ selectedTab.name }}
    .content-title__dismiss(*ngIf="closable", (click)="dismiss.emit()")

